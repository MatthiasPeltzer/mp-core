# TYPO3 layout options content elements
tt_content {
  stdWrap {
    innerWrap {
      cObject = CASE
      cObject {
        key.field = layout

        1 = TEXT
        1.value = <div class="row"><div class="col-12 offset-xl-2 col-xl-8">|</div></div>

        2 = TEXT
        2.value = <div class="row"><div class="col-12 offset-xl-1 col-xl-10">|</div></div>
      }
    }
  }

  htmltables_htmltable {
    settings {
      useBootstrapTableClass = 1
      useResponsiveTable = 1
    }
  }
}
